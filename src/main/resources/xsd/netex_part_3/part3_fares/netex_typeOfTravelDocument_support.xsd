      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="TypeOfMachineReadabilityIdType">
    <xsd:restriction base="TypeOfValueIdType"/>
  </xsd:simpleType>
  <xsd:element name="TypeOfMachineReadabilityRef" type="TypeOfMachineReadabilityRefStructure" substitutionGroup="TypeOfEntityRef"/>
  <xsd:complexType name="TypeOfMachineReadabilityRefStructure">
    <xsd:simpleContent>
      <xsd:restriction base="TypeOfValueRefStructure">
        <xsd:attribute name="ref" type="TypeOfMachineReadabilityIdType" use="required"/>
        <xsd:attribute name="nameOfRefClass" type="NameOfClassTypeOfMachineReadabilityRefStructureType" default="TypeOfMachineReadability"/>
        <xsd:attribute name="modification" type="ModificationEnumeration" use="optional"/>
        <xsd:attribute name="versionRef" type="VersionIdType" use="optional"/>
        <xsd:attribute name="uri" type="xsd:anyURI" use="optional"/>
        <xsd:attributeGroup ref="ReferenceModificationDetailsGroup"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
