  <!-- ===ENTIITY IN VERSION IN FRAME====Used in FARE FRAME)=================================================== -->
  <!-- ===ENTIITY IN VERSION IN FRAME====Used in FARE FRAME)=================================================== -->
  <xsd:group name="FareProductsInFrameGroup">
    <xsd:sequence>
      <xsd:element name="usageParameters" type="usageParametersInFrame_RelStructure" minOccurs="0"/>
      <xsd:element name="accessRightParameterAssignments" type="accessRightParameterAssignmentsInFrame_RelStructure" minOccurs="0"/>
      <xsd:element name="fareProducts" type="fareProductsInFrame_RelStructure" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="fareProductsInFrame_RelStructure">
    <xsd:complexContent>
      <xsd:extension base="frameContainmentStructure">
        <xsd:sequence>
          <xsd:element ref="FareProduct_Dummy" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ====FARE PRODUCT=================================================== -->
  <xsd:element name="ServiceAccessRight_Dummy" type="DataManagedObjectStructure" abstract="true" substitutionGroup="PriceableObject_Dummy"/>
  <xsd:element name="ServiceAccessRight" substitutionGroup="ServiceAccessRight_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="ServiceAccessRight_VersionStructure">
          <xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="DataManagedObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ServiceAccessRightGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="ServiceAccessRightIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="ServiceAccessRight_VersionStructure">
    <xsd:complexContent>
      <xsd:extension base="PriceableObject_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="ServiceAccessRightGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="ServiceAccessRightGroup">
    <xsd:sequence>
      <xsd:group ref="ProductDescriptionGroup"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="ProductDescriptionGroup">
    <xsd:sequence>
      <xsd:element ref="PrivateCode" minOccurs="0"/>
