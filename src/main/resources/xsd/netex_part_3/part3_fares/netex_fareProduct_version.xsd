            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="DataManagedObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ServiceAccessRightGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="FareProductGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="SaleDiscountRightGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="SaleDiscountRightIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="SaleDiscountRight_VersionStructure">
    <xsd:complexContent>
      <xsd:extension base="FareProduct_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="SaleDiscountRightGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="SaleDiscountRightGroup">
    <xsd:sequence>
      <xsd:element name="ProductType" type="SaleDiscountRightEnumeration" minOccurs="0"/>
      <xsd:group ref="OtherProductGroup" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="OtherProductGroup">
    <xsd:choice>
      <xsd:element ref="GeneralGroupOfEntitiesRef"/>
      <xsd:element ref="GeneralGroupOfEntities"/>
    </xsd:choice>
  </xsd:group>
  <!-- ==== ENTITLEMENT PRODUCT.=============================================== -->
  <xsd:element name="EntitlementProduct" substitutionGroup="ServiceAccessRight_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="EntitlementProduct_VersionStructure">
          <xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="DataManagedObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ServiceAccessRightGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntitlementProductGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="EntitlementProductIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="EntitlementProduct_VersionStructure">
    <xsd:complexContent>
      <xsd:extension base="ServiceAccessRight_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="EntitlementProductGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="EntitlementProductGroup">
    <xsd:sequence>
      <xsd:element ref="GeneralOrganisationRef" minOccurs="0"/>
      <xsd:element name="prices" type="fareProductPrices_RelStructure" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <!-- ==== THIRD PARTY PRODUCT.=============================================== -->
  <xsd:element name="ThirdPartyProduct" substitutionGroup="FareProduct_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="ThirdPartyProduct_VersionStructure">
          <xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="DataManagedObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ServiceAccessRightGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="FareProductGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ThirdPartyProductGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="ThirdPartyProductIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="ThirdPartyProduct_VersionStructure">
    <xsd:complexContent>
      <xsd:extension base="FareProduct_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="ThirdPartyProductGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="ThirdPartyProductGroup">
    <xsd:sequence>
      <xsd:group ref="OtherProductGroup" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <!-- ==== USAGE DISCOUNT RIGHT.=============================================== -->
  <xsd:element name="UsageDiscountRight" substitutionGroup="FareProduct_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="UsageDiscountRight_VersionStructure">
          <xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="DataManagedObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ServiceAccessRightGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="FareProductGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="UsageDiscountRightGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="UsageDiscountRightIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:group name="UsageDiscountRightGroup">
    <xsd:sequence>
      <xsd:element name="ProductType" type="UsageDiscountRightEnumeration" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <!-- ==== CAPPED DISCOUNT RIGHT.=============================================== -->
  <xsd:element name="CappedDiscountRight" substitutionGroup="FareProduct_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="CappedDiscountRight_VersionStructure">
          <xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="DataManagedObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ServiceAccessRightGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="FareProductGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="SaleDiscountRightGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="CappedDiscountRightGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="CappedDiscountRightIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="CappedDiscountRight_VersionStructure">
    <xsd:complexContent>
      <xsd:extension base="SaleDiscountRight_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="CappedDiscountRightGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="CappedDiscountRightGroup">
    <xsd:sequence>
      <xsd:element name="cappingRules" type="cappingRules_RelStructure" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <!-- ==== CAPPING RULE.=============================================== -->
  <xsd:complexType name="cappingRules_RelStructure">
    <xsd:complexContent>
      <xsd:extension base="strictContainmentAggregationStructure">
        <xsd:sequence>
          <xsd:element ref="CappingRule" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="CappingRule" substitutionGroup="PriceableObject_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="CappingRule_VersionedChildStructure">
          <xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:sequence>
                <xsd:element ref="keyList" minOccurs="0"/>
                <xsd:element ref="Extensions" minOccurs="0"/>
                <xsd:element ref="BrandingRef" minOccurs="0"/>
              </xsd:sequence>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="CappingRuleGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="CappingRuleIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="CappingRule_VersionedChildStructure">
    <xsd:complexContent>
      <xsd:extension base="PriceableObject_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="CappingRuleGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="CappingRuleGroup">
    <xsd:sequence>
      <xsd:element name="MaximumDistance" type="DistanceType" minOccurs="0"/>
      <xsd:element name="CappingPeriod" type="CappingPeriodEnumeration" default="day" minOccurs="0"/>
      <xsd:element ref="CappedDiscountRightRef" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="PreassignedFareProductRef" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="ValidableElementRef" minOccurs="0"/>
      <xsd:group ref="OptimisedOneManyAssignmentGroup" minOccurs="0"/>
      <xsd:element name="prices" type="cappingRulePrices_RelStructure" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="CappingStartConstraintGroup">
    <xsd:sequence>
      <xsd:element name="CappingStartConstraintType" type="CappingPeriodStartConstraintTypeEnumeration" minOccurs="0"/>
      <xsd:element name="startOnlyOn" minOccurs="0">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="dayTypes_RelStructure">
              <xsd:sequence>
                <xsd:element name="StartMonth" type="xsd:gMonth" minOccurs="0"/>
                <xsd:element name="StartDay" type="xsd:gDay" minOccurs="0"/>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:group>
  <!-- ==== AMOUNT OF PRICE UNIT PRODUCT.=============================================== -->
  <xsd:complexType name="UsageDiscountRight_VersionStructure">
    <xsd:complexContent>
      <xsd:extension base="FareProduct_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="UsageDiscountRightGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="AmountOfPriceUnitProduct" substitutionGroup="FareProduct_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="AmountOfPriceUnitProduct_VersionStructure">
          <xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="DataManagedObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ServiceAccessRightGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="FareProductGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="AmountOfPriceUnitProductGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="AmountOfPriceUnitProductIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="AmountOfPriceUnitProduct_VersionStructure">
    <xsd:complexContent>
      <xsd:extension base="FareProduct_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="AmountOfPriceUnitProductGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="AmountOfPriceUnitProductGroup">
    <xsd:sequence>
      <xsd:element name="ProductType" type="AmountOfPriceUnitEnumeration" minOccurs="0"/>
      <xsd:element ref="PriceUnitRef" minOccurs="0"/>
      <xsd:element name="Amount" type="xsd:decimal" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>
  <!-- ==== PREASSIGNED FARE PRODUC=============================================== -->
  <xsd:element name="PreassignedFareProduct" substitutionGroup="FareProduct_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="PreassignedFareProduct_VersionStructure">
          <xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="EntityInVersionGroup" minOccurs="0"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="DataManagedObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PriceableObjectGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="ServiceAccessRightGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="FareProductGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="PreassignedFareProductGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="PreassignedFareProductIdType" use="required"/>
