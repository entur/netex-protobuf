  <xsd:simpleType name="ObservedPassingTimeIdType">
    <xsd:restriction base="PassingTimeIdType"/>
  </xsd:simpleType>
  <xsd:element name="ObservedPassingTimeRef" type="ObservedPassingTimeRefStructure" substitutionGroup="PassingTimeRef"/>
