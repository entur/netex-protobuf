  <xsd:element name="PointOfInterestEntrance" substitutionGroup="Entrance">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="PointOfInterestEntrance_VersionStructure">
          <xsd:sequence>
