  </xsd:group>
  <!-- =====  TAXI SERVICE ============================================= -->
  <xsd:element name="TaxiService" substitutionGroup="VehiclePoolingService_Dummy">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="TaxiService_VersionStructure">
          <xsd:sequence>
