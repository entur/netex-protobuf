              <xsd:group ref="EquipmentGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="MobilityServiceGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="CommonVehicleServiceGroup"/>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:group ref="VehiclePoolingServiceGroup"/>
