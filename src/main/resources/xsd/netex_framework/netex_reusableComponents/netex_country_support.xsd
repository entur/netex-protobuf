  <!-- =====Country================================================================== -->
  <xsd:simpleType name="CountryCodeType">
    <xsd:restriction base="IanaCountryTldEnumeration">
      <xsd:maxLength value="2"/>
      <xsd:minLength value="2"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CountryPrincipalityCodeType">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:element name="CountryRef">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="CountryRefStructure"/>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="CountryRefStructure">
    <xsd:simpleContent>
      <xsd:extension base="CountryPrincipalityCodeType">
        <xsd:attribute name="ref" type="CountryCodeType"/>
        <xsd:attribute name="refPrincipality" type="CountryPrincipalityCodeType"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="countryRefs_RelStructure">
    <xsd:complexContent>
      <xsd:extension base="oneToManyRelationshipStructure">
        <xsd:sequence>
          <xsd:element ref="CountryRef" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="AllCountriesRef">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="AllCountriesRefStructure"/>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="AllCountriesRefStructure">
    <xsd:simpleContent>
      <xsd:restriction base="TypeOfValueRefStructure">
        <xsd:attribute name="ref" type="TypeOfValueIdType" use="required" fixed="All"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:group name="OneOrAllCountriesRefGroup">
    <xsd:choice>
      <xsd:element ref="AllCountriesRef"/>
      <xsd:element ref="CountryRef"/>
    </xsd:choice>
  </xsd:group>
