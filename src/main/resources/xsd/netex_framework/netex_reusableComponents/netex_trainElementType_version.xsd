            <xsd:sequence>
              <xsd:group ref="TrailingElementTypeGroup"/>
            </xsd:sequence>
          </xsd:sequence>
          <xsd:attribute name="id" type="TrailingElementTypeIdType" use="required"/>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="TrailingElementType_VersionStructure">
    <xsd:complexContent>
      <xsd:extension base="TrainElementType_VersionStructure">
        <xsd:sequence>
          <xsd:group ref="TrailingElementTypeGroup"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:group name="TrailingElementTypeGroup">
    <xsd:sequence/>
  </xsd:group>
  <!-- ======================================================================= -->
</xsd:schema>
