      <xsd:extension base="OtherOrganisation_VersionStructure"/>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ======================================================================= -->
  <xsd:element name="ServicedOrganisation" substitutionGroup="Organisation_Dummy">
